{
  "manifest_version": 3,
  "name": "Sample Web Extension",
  "version": "0.0.1",
  "background": {
    "service_worker": "background/index.ts",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "Default Title",
    "default_popup": "popup.html",
    "default_icon": {}
  },
  "permissions": ["storage", "proxy", "webRequest", "tabs"],
  "host_permissions": ["<all_urls>"]
}
